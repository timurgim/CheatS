local Tab = Window:MakeTab({
	Name = "Settings",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Close script",
	Callback = function()
	OrionLib:Destroy()
  	end    
})

Tab:AddLabel("If you notice a bug or want to suggest an idea,")
Tab:AddLabel("then write to me in discord : o5_3counsil")




